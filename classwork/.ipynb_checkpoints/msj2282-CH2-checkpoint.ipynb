    "import numpy as np\n",
    "num = 2025\n",
    "final = np.array([])\n",
    "while(num>1):\n",
    "    if(num%2==1):\n",
    "        final = np.insert(final, 0,1)\n",
    "        num = (num-1)/2\n",
    "    else:\n",
    "        final = np.insert(final, 0, 0)\n",
    "        num = num/2\n",
    "print(final)"
